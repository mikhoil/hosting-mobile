{
  "expo": {
		"name": "hosting-mobile",
		"slug": "hosting-mobile",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./src/app-flat/assets/images/logo-green.png",
		"scheme": "myapp",
		"userInterfaceStyle": "automatic",
		"splash": {
			"image": "./src/app-flat/assets/images/logo-green.png",
			"resizeMode": "contain",
			"backgroundColor": "#ffffff"
		},
		"assetBundlePatterns": ["**/*"],
		"ios": {
			"supportsTablet": true
		},
		"android": {
			"adaptiveIcon": {
				// "foregroundImage": "./assets/images/adaptive-icon.png",
				"backgroundColor": "#ffffff"
			}
		},
		"web": {
			"bundler": "metro",
			"output": "static"
			// "favicon": "./src/app-flat/assets/images/logo-green.png"
		},
		"plugins": ["expo-router"],
		"experiments": {
			"typedRoutes": true,
			"tsconfigPaths": true
		},
		"packagerOpts": {
			"config": "metro.config.js",
			"sourceExts": ["js", "jsx", "cjs", "css"]
		}
	}
}
